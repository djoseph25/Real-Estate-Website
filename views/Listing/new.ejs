<%- include ('partials/showNavbar')  %> 
    <section class="wizard-section">
		<div class="row no-gutters">
			<div class="col-lg-6 col-md-6">
				<div class="wizard-content-left d-flex justify-content-center align-items-center">
					<h1>Selling your home have never been easier</h1>
                    
				</div>
			</div>
			<div class="col-lg-6 col-md-6">
				<div class="form-wizard">
					<form action="/listing" method="post" role="form"  enctype="multipart/form-data">
						<div class="form-wizard-header mt-5 pt-5">
							<p>Fill all form field to go next step</p>
							<ul class="list-unstyled form-wizard-steps clearfix">
								<li class="active"><span>1</span></li>
								<li><span>2</span></li>
								<li><span>3</span></li>
								<li><span>4</span></li>
							</ul>
						</div>
                        	<fieldset class="wizard-fieldset show">
							<h5>Personal Information</h5>
							<div class="form-group">
								<input type="text" class="form-control wizard-required" id="location" name='location'>
								<label for="location" class="wizard-form-text-label">Location*</label>
								<div class="wizard-form-error"></div>
							</div>
							<div class="form-group">
								<select class="form-control wizard-required" id="property" name='property'>
                                    <option value="" >Select property type...</option>
                                    <option value="House">House</option>
                                    <option value="Condo">Condo</option>
                                    <option value="Townhome">Townhome</option>
                                    <option value="Duplex">Duplex</option>
                                </select>
								<!-- <label for="lname" class="wizard-form-text-label">Last Name*</label> -->
								<div class="wizard-form-error"></div>
							</div>
                            <div class="form-group">
								<select class="form-control wizard-required" id="status" name='status'>
                                    <option value="" >Select Status....</option>
                                    <option value="Rent">Rent</option>
                                    <option value="Sale">Sale</option>
                                </select>
								<!-- <label for="lname" class="wizard-form-text-label">Last Name*</label> -->
								<div class="wizard-form-error"></div>
							</div>
							
                            <div class="form-group clearfix">
								<a href="javascript:;" class="form-wizard-next-btn float-right">Next</a>
							</div>
						</fieldset>	
                        <!-- STEP @ 2 -->
						<fieldset class="wizard-fieldset">
							<h5>Account Information</h5>
                            <div class="form-group">
								<select class="form-control wizard-required" id="bedroom" name='bedroom'>
                                    <option value="" >Select amount Bedroom...</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                    <option value="4">4</option>
                                    <option value="5">5</option>
                                    <option value="6">6</option>
                                    <option value="7">7</option>
                                </select>
								<div class="wizard-form-error"></div>
							</div>
                            <div class="form-group">
								<select class="form-control wizard-required" id="bathroom" name='bathroom'>
                                    <option value="" >Select amount Bathroom...</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                    <option value="4">4</option>
                                    <option value="5">5</option>
                                    <option value="6">6</option>
                                    <option value="7">7</option>
                                </select>
								<!-- <label for="lname" class="wizard-form-text-label">Last Name*</label> -->
								<div class="wizard-form-error"></div>
							</div>
                            <div class="form-group">
								<select class="form-control wizard-required" id="garage" name='garage'>
                                    <option value="" >Select amount Garage..</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                    <option value="4">4</option>
                                </select>
								<!-- <label for="lname" class="wizard-form-text-label">Last Name*</label> -->
								<div class="wizard-form-error"></div>
							</div>
                            <div class="form-group clearfix">
								<a href="javascript:;" class="form-wizard-previous-btn float-left">Previous</a>
								<a href="javascript:;" class="form-wizard-next-btn float-right">Next</a>
							</div>
						
						</fieldset>	
                        <!-- STEP @ 3-->
						<fieldset class="wizard-fieldset">
							<h5>Genereal Information</h5>
							<div class="form-group">
								<input type="number" class="form-control wizard-required" id="area" name='area'>
								<label for="area" class="wizard-form-text-label">SquareFeet</label>
								<div class="wizard-form-error"></div>
							</div>
                            <div class="form-group">
								<input type="number" class="form-control wizard-required" id="price" name='price'>
								<label for="price" class="wizard-form-text-label" >price</label>
								<div class="wizard-form-error"></div>
							</div>
                        
							<div class="form-group">
								<!-- <input type="text" class="form-control wizard-required" id="image" name='image'> -->
                <input type="file" name="image" id='image' multiple>
                <!-- <label for="image" class="wizard-form-text-label">Image</label> -->
								<div class="wizard-form-error"></div>
							</div>
						
							<div class="form-group clearfix">
								<a href="javascript:;" class="form-wizard-previous-btn float-left">Previous</a>
								<a href="javascript:;" class="form-wizard-next-btn float-right">Next</a>
							</div>
						</fieldset>	
                        <!--SECTION STEP @ 4 -->
						<fieldset class="wizard-fieldset">
							<h5>Property Information</h5>
								<div class="form-group">
								<textarea type="text" class="form-control wizard-required" id="description" name='description' rows="6"></textarea>
								<label for="description" class="wizard-form-text-label">Please Add a detail description*</label>
								<div class="wizard-form-error"></div>
							</div>
							
							<div class="form-group clearfix">
								<a href="javascript:;" class="form-wizard-previous-btn float-left">Previous</a>
								<button href="javascript:;" class="form-wizard-submit float-right">Submit</button>
							</div>
						</fieldset>	
					</form>
				</div>
			</div>
		</div>
	</section>
	
